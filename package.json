{
  "name": "build-ts",
  "version": "0.0.0-semantically-released",
  "repository": "github:WillBooster/build-ts",
  "license": "UNLICENSED",
  "author": "WillBooster Inc.",
  "type": "module",
  "bin": "bin/index.mjs",
  "files": [
    "bin/",
    "dist/",
    "babel.*"
  ],
  "scripts": {
    "build": "yarn start build -t app",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{__tests__,scripts,src}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "node --no-warnings --loader ts-node/esm src/index.ts",
    "start-prod": "yarn build && yarn build-ts",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "24.0.0",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "0.3.0",
    "@rollup/pluginutils": "5.0.2",
    "babel-plugin-polyfill-corejs3": "0.6.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "core-js": "3.27.1",
    "core-js-pure": "3.27.1",
    "dotenv": "16.0.3",
    "rollup": "3.9.1",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-node-externals": "5.1.0",
    "rollup-plugin-string": "3.0.0",
    "rollup-plugin-ts": "3.1.1",
    "ts-node": "10.9.1",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/babel__core": "7.1.20",
    "@types/babel__plugin-transform-runtime": "7.9.2",
    "@types/babel__preset-env": "7.9.2",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.18",
    "@types/yargs": "17.0.19",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@willbooster/babel-configs": "1.3.0",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.5.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.2",
    "semantic-release": "20.0.2",
    "sort-package-json": "2.1.0",
    "type-fest": "3.5.1",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@4.0.0-rc.35",
  "engines": {
    "node": ">=16.19.0"
  }
}
